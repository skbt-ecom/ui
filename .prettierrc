{
  "arrowParens": "always",
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "singleQuote": true,
  "trailingComma": "none",
  "vueIndentScriptAndStyle": false,
  "printWidth": 130,
  "tabWidth": 2,
  "rangeStart": 0,
  "endOfLine": "auto",
  "semi": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
